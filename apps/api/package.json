{
  "name": "@mag/api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node src/index.js",
    "dev:debug": "ts-node-dev --inspect --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node src/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "prisma:gen": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=./prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./prisma/schema.prisma",
    "prisma:format": "prisma format --schema=./prisma/schema.prisma",
    "start:dev:3001": "cross-env PORT=3001 node src/index.js",
    "postinstall": "prisma generate --schema=./prisma/schema.prisma",
    "start:dev": "node src/index.js",
    "dev:watch": "cross-env PORT=3001 nodemon src/index.js"
  },
  "dependencies": {
    "@prisma/client": "^6.16.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^6.2.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "mag-webapp-v8": "file:../..",
    "morgan": "^1.10.1",
    "pino": "^9.3.2",
    "pino-http": "^10.2.0",
    "prom-client": "^15.1.2",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.12",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.9.0",
    "express-list-endpoints": "^7.1.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.3.3",
    "prisma": "^6.16.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.5",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
